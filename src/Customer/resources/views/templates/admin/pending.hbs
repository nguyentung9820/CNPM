<h1>Pending Request</h1>
<div>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>#</th>
                <th>Name</th>
                <th>Group</th>
                <th>Create At</th>
                <th>Action</th>
        </thead>
        {{#each pendings}}
        <tbody>
            <tr>
                <th>{{@index}}</th>
                <th>{{this.username}}</th>
                <th>{{this.customer_group}}</th>
                <th>{{this.createdAt}}</th>
                <th>
                    <a href="/management/customer/pending/approve/{{this._id}}"><button
                            class="btn btn-success">Approve</button></a>
                    <a href="/management/customer/pending/cancel/{{this._id}}"><button
                            class="btn btn-dark">Cancel</button></a>
                </th>

            </tr>
        </tbody>
        {{/each}}
    </table>
</div>